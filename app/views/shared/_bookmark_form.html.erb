<%= form_for @bookmark do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
<div>
    <table><tr><td>URL:</td><td><%= f.text_field :url %></td></tr>
    <tr><td>Name:</td><td><%= f.text_field :name %></td></tr>
    </table>
</div>
  <div>
    <%= f.submit "Submit" %>
  </div>
<% end %>
